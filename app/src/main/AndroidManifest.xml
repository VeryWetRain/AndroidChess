<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidchess91">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidChess91">
        <activity
            android:name=".replay"
            android:label="Replay"
            android:parentActivityName=".Records"
            android:theme="@style/Theme.AndroidChess91.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androidchess91.Records" />
        </activity>
        <activity
            android:name=".Records"
            android:label="Recorded Games"
            android:parentActivityName=".Homepage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androidchess91.Homepage" />
        </activity>
        <activity
            android:name=".Chess"
            android:label="@string/title_activity_play"
            android:parentActivityName=".Homepage"
            android:theme="@style/Theme.AndroidChess91.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.androidchess91.Homepage" />
        </activity>
        <activity android:name=".Homepage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>